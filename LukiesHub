local Button = Tab:CreateButton({
   Name = "Instant Prompt",
   Callback = function()
      Rayfield:Notify({
   Title = "Successfully Made instant",
   Content = "Makes E interactions instant.",
   Duration = 6.5,
   Image = "key",
})
local ProximityPromptService = game:GetService("ProximityPromptService")
local instantInteractEnabled = true

ProximityPromptService.PromptButtonHoldBegan:Connect(function(prompt, player)
    if instantInteractEnabled then
        fireproximityprompt(prompt)
    end
end)
   end,
})
local Button = Tab:CreateButton({
   Name = "Unlock Fps",
   Callback = function()
   Rayfield:Notify({
   Title = "Successfully Unlocked",
   Content = "Removed the 60 FPS cap.",
   Duration = 6.5,
   Image = "key",
})
   setfpscap(999999999999)
   end,
})
task.wait(8.5)
Rayfield:Notify({
   Title = "Luk.z Said",
   Content = "Unsure of what the 1st/2nd/3rd mean? meaning = script rank",
   Duration = 10,
   Image = "moon",
})
local questions = {
    "Enjoying our script? Join our discord!",
    "Thank you for using our script!",
    "Got suggestions or feedback? Drop by our Discord and let us know!",
}
while true do
    for i = 1, #questions do
        task.wait(75)

        Rayfield:Notify({
            Title = "Luk.z",
            Content = questions[i],
            Duration = 9,
            Image = "moon",
        })
    end
end
